class Solution {
    public int lengthOfLastWord(String s) {
        String str =s.trim();
        int count=0;
        int len=str.length()-1;
        for(int i =len; i>=0; --i){
            if(str.charAt(i) == ' '){
                break; 
            }
            count++;
        }     
        return count;  
    }
}
