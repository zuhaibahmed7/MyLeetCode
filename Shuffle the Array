class Solution {
    public int[] shuffle(int[] nums, int n) {
        int len =nums.length;
        int j =0; 
        int half=len/2;
        int []result= new int[len];
        for(int i =0; i<half; ++i){
            result[i+j]=nums[i];
            j++;
            result[j+i]= nums[n+i];
        }
        return result;
    }
}
