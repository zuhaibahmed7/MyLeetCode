class Solution {
    public int maximum69Number (int num) {
        char[] chr = String.valueOf(num).toCharArray();
        for(int i = 0; i<chr.length;++i){
            if(chr[i] =='6'){
                chr[i] = '9';
                break;
            }
        }
        return Integer.parseInt(new String(chr));
    }
}
